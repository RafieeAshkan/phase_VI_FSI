FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 7 0);

boundaryField
{

	
    inlet
    {
        type            fixedValue;
        value           uniform (0 7 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
	"blade.*"
    {
        type            newMovingWallVelocity;
        value           uniform (0 0 0);
    }
	hub
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


